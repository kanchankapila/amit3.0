{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/stockinsights2.ico",
          "/index.html",
          "/manifest.json",
          "/*.css"
        ],
        "urls": [
          "/main.*.js",
          "/polyfills.*.js",
          "/runtime.*.js",
          "/vendor.*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "app-data",
      "urls": [],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "7d",
        "strategy": "freshness"
      }
    },
    {
      "name": "api-cache",
      "urls": [
        "/.netlify/functions/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1h",
        "timeout": "10s"
      }
    }
  ]
}
